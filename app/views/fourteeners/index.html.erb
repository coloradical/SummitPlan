<!DOCTYPE html>
<html>
	<head>
		<style>
			#map {
				width: 100%;
				height: 500px;
				background-color: grey;
			}
		</style>
	</head>
	<body>
		<h3>Summit Plan</h3>
		<div id="map"></div>
		<script>
			function initMap() {
				var elbert = {lat: 39.1178, lng: -106.4452};
				var map = new google.maps.Map(document.getElementById('map'), {
					zoom: 7,
					center: elbert
				});
				var marker = new google.maps.Marker({
					position: elbert,
					map: map
				});
				var marker2 = new google.maps.Marker({
					position: {lat: 39.582638, lng: -105.66861},
					map: map
				});
				var fourteener = $('.temp_information').data('show');
			}
		</script>
		<script async defer
		src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBBYkGDkbDQBSWK4WoQM-r8tA5WXkqu_Rw&callback=initMap">
		</script>
	</body>
</html>